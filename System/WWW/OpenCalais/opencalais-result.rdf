  <rdf:Description c:calaisRequestID="2925f83f-6027-4a9d-b04c-05486435facc" c:id="http://id.opencalais.com/DaxRTB0ATC2qsIfmx-uDuQ" rdf:about="http://d.opencalais.com/dochash-1/44dee71d-3f27-3bb4-b7fe-9e6ecd25ae1d">
